# Local PostgreSQL configuration
POSTGRES_DB=your_local_db_name
POSTGRES_USER=your_local_db_user
POSTGRES_PASSWORD=your_local_db_password
POSTGRES_HOST=localhost
POSTGRES_PORT=5432

# Azure PostgreSQL configuration
AZURE_POSTGRES_DB=your_azure_db_name
AZURE_POSTGRES_USER=your_azure_db_user
AZURE_POSTGRES_PASSWORD=your_azure_db_password
AZURE_POSTGRES_HOST=your_azure_db_host
AZURE_POSTGRES_PORT=5432

# Azure Container Registry configuration
AZURE_ACR_NAME=your_acr_name
AZURE_ACR_LOGIN_SERVER=your_acr_login_server
AZURE_ACR_USERNAME=your_acr_username
AZURE_ACR_PASSWORD=your_acr_password

# Azure App Service Plan configuration
AZURE_APP_SERVICE_PLAN=your_app_service_plan
AZURE_WEB_APP_NAME=your_web_app_name
